?
	->
		(infant-activity{moving-toward})
		?
			<!>
				(infant-orientation{toward})
			<!>
				(infant-robot-distance{direct-social-interaction})
		[spin]
	->
		?
			->
				(infant-robot-distance{solitary-play})
				<!>
					(infant-orientation{away})
			->
				<!>
					(infant-activity{moving-away})
				<!>
					(infant-orientation{toward})
				<!>
					(infant-robot-distance{direct-social-interaction})
		[move-toward]
	->
		<!>
			(infant-robot-distance{social-interaction})
		?
			(infant-activity{stationary})
			->
				<!>
					(infant-activity{moving-toward})
				<!>
					(infant-orientation{away})
		[move-away]
	->
		(has-bubbles{x})
		<!>
			(infant-activity{moving-toward})
		<!>
			(infant-orientation{away})
		[blow-bubbles]
	->
		<!>
			(infant-activity{moving-toward})
		<!>
			(infant-orientation{away})
		[refill-bubbles]
	->
		<!>
			(infant-activity{moving-toward})
		[lights-and-sounds]
	[keep-away]
